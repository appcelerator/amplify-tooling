{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"module": "es2022",
		"moduleResolution": "node",
		// "noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "./dist",
		"resolveJsonModule": true,
		"sourceMap": true,
		// "strict": true,
		"target": "es2022",
	},
	"include": [ "./src/**/*" ]
}
