{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"module": "nodenext",
		"moduleResolution": "nodenext",
		// "noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "./dist",
		"resolveJsonModule": true,
		"sourceMap": true,
		// "strict": true,
		"target": "es2024",
	},
	"include": [ "./src/**/*" ]
}
