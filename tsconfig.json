{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"lib": [
			"ES2019"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": true,
		"target": "ES2019"
	},
	"exclude": [ "node_modules" ]
}
