{
  "name": "@axway/amplify-request",
  "version": "4.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "HTTP request library for Axway CLI packages",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Axway, Inc. <npmjs@appcelerator.com>",
  "maintainers": [
    "Chris Barber <cbarber@axway.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "axway",
    "amplify",
    "request",
    "got",
    "http",
    "https",
    "proxy"
  ],
  "scripts": {
    "build": "tsc",
    "coverage": "c8 npm run test",
    "lint": "concurrently -s all -m 1 npm:lint:src npm:lint:test",
    "lint:src": "eslint src/**/*.ts",
    "lint:test": "eslint --parser-options=project:'test/tsconfig.json' test/**/*.ts",
    "prepublishOnly": "concurrently -s all -m 1 npm:lint:src npm:build",
    "test": "ts-mocha -n loader=ts-node/esm test/**/test-*.ts --reporter spec"
  },
  "dependencies": {
    "@axway/amplify-utils": "^1.0.9",
    "got": "^12.1.0",
    "http-proxy-agent": "^5.0.0",
    "https-proxy-agent": "^5.0.1",
    "pretty-bytes": "^6.0.0",
    "snooplogg": "^5.0.0"
  },
  "homepage": "https://github.com/appcelerator/amplify-tooling#readme",
  "bugs": "https://github.com/appcelerator/amplify-tooling/issues",
  "repository": "https://github.com/appcelerator/amplify-tooling/tree/master/packages/amplify-request",
  "engines": {
    "node": ">=14.15.0"
  }
}
