{
  "name": "axway",
  "version": "4.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A unified CLI for the Axway Amplify platform.",
  "type": "module",
  "exports": "./dist/index.js",
  "author": "Axway, Inc. <npmjs@appcelerator.com>",
  "maintainers": [
    "Chris Barber <cbarber@axway.com>",
    "Ewan Harris <eharris@axway.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "axway",
    "amplify"
  ],
  "bin": {
    "axway": "./bin/axway"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "tsc",
    "coverage": "c8 npm run test",
    "lint": "concurrently -s all -m 1 npm:lint:src npm:lint:test npm:lint:tsc",
    "lint:src": "eslint src/**/*.ts",
    "lint:test": "eslint --parser-options=project:'test/tsconfig.json' test/**/*.ts",
    "lint:tsc": "tsc -p tsconfig.json",
    "prepublishOnly": "concurrently -s all -m 1 npm:lint:src npm:build",
    "test": "ts-mocha -n loader=ts-node/esm test/**/test-*.ts --require ../../test/setup.js --reporter spec"
  },
  "dependencies": {
    "@axway/amplify-cli-utils": "^5.0.9",
    "@axway/amplify-utils": "^1.0.9",
    "@axway/axway-cli-auth": "^3.3.5",
    "@axway/axway-cli-oum": "^2.0.9",
    "@axway/axway-cli-pm": "^4.0.9",
    "boxen": "^7.0.0",
    "check-kit": "^2.0.0",
    "cli-kit": "^1.16.0",
    "serialize-error": "^11.0.0",
    "snooplogg": "^5.0.0",
    "v8-compile-cache": "^2.3.0"
  },
  "homepage": "https://github.com/appcelerator/amplify-tooling#readme",
  "bugs": "https://github.com/appcelerator/amplify-tooling/issues",
  "repository": "https://github.com/appcelerator/amplify-tooling/tree/master/packages/axway-cli",
  "engines": {
    "node": ">=14.15.0"
  }
}
